<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="AMSPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>reg.Login</class>
    <class>reg.Society</class>
    <class>reg.Sadmin</class>
    <class>reg.SocietyMembers</class>
    <class>reg.Apartment</class>
    <class>reg.Complain</class>
    <class>reg.Notice</class>
    <class>reg.Album</class>
    <class>reg.PicGallery</class>
    <class>reg.CalMaintenance</class>
    <class>reg.PaidMaintenance</class>
    <class>reg.Transaction</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/ams?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="meet123"/>
    </properties>
  </persistence-unit>
</persistence>
